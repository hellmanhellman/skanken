<div id="contactedFormModel" class="modal fade bs-job-modal-sm" tabindex="-1" role="dialog" aria-labelledby="mySmallModalLabel" aria-hidden="true">
  <div class="modal-dialog modal-sm">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
        <h4 class="modal-title"><%= t('member.contacted') %></h4>
      </div>
      <div class="modal-body">
        <div id="modal_form_contacteds">
          <%= simple_form_for :contacted do |f| %>
            <%= f.input :activity_id, label: t('member.activity'), required: true, collection: @cooperative.activities %>
            <%= f.button :submit, t('job.form.submit')  %>
          <% end %>
        </div>
      </div>
    </div>
  </div>
</div>